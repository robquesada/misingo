<% pet = lost_report.pet %>
<center>
  <label>Compartir en redes sociales</label>
  <%= render partial: 'shared/social_shares',
      locals: { url: pet_lost_report_url(pet) } %>
  <div class='row final'></div>
  <%= link_to(pet_lost_report_flyer_path(pet), class: 'small button', target: '_blank') do %>
    Imprimir
  <% end %>
  <div class="flyer-preview-wrap">
    <iframe class="flyer-preview-frame", scrolling="no" src="extravios/cartel"></iframe>
  </div>
  <div class='row final'></div>
  <p><label>Ellos también  asociaciones cerca de <%= lost_report.province.name %> te pueden ayudar con tu búsqueda:</label></p>
  <dl class='accordion' data-accordion>
    <dd class='accordion-navigation'>
      <a href='#panel_associations', class='accordion'>Ver todas</a>
      <div id='panel_associations', class='content'>
        <p class='marginless'>Pronto tendremos disponible esta funcionalidad.</p>
      </div>
    </dd>
  </dl>
</center>
